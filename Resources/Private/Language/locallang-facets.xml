<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>
			Language labels for facet content.
			The file »locallang-facets.xml« will be loaded from the path set as
			»view.languageRootPath« in TypoScript.

			Localisation keys / label indexes follow the convention:
				* facet name for index FOO: facet.FOO
				* facet name for value BAR in index FOO: facet.FOO.BAR
		</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="facet.typ">Type</label>
			<label index="facet.typ.formular">Text</label>
			<label index="facet.typ.gott">God</label>
			<label index="facet.typ.ort">Location</label>
			<label index="facet.typ.word">Word</label>

			<label index="facet.band">Volume</label>
			<label index="facet.band.1">I</label>
			<label index="facet.band.2">II</label>
			<label index="facet.band.3">III</label>
			<label index="facet.band.4">IV</label>
			<label index="facet.band.5">V</label>
			<label index="facet.band.6">VI</label>
			<label index="facet.band.7">VII</label>
			<label index="facet.band.8">VIII</label>

		</languageKey>

		<languageKey index="de" type="array">
			<label index="facet.typ">Typ</label>
			<label index="facet.typ.formular">Text</label>
			<label index="facet.typ.gott">Gott</label>
			<label index="facet.typ.ort">Ort</label>
			<label index="facet.typ.word">Wort</label>

			<label index="facet.band">Band</label>
			<label index="facet.band.1">I</label>
			<label index="facet.band.2">II</label>
			<label index="facet.band.3">III</label>
			<label index="facet.band.4">IV</label>
			<label index="facet.band.5">V</label>
			<label index="facet.band.6">VI</label>
			<label index="facet.band.7">VII</label>
			<label index="facet.band.8">VIII</label>

		</languageKey>
	</data>
</T3locallang>
